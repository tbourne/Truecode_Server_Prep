## This section of the code will install all the required packages for Trudecode to run on The Time for Monkeys 
## Group Servers. 

## Preform An Yum Update 
yum update -y 


#install the base packages 
yum install cpio.x86_64 -yyum install cracklib.x86_64 -yyum install cracklib-dicts.x86_64 -yyum install curl.x86_64 -yyum install cyrus-sasl.x86_64 -yyum install cyrus-sasl-lib.x86_64 -yyum install db4.x86_64 -yyum install db4-utils.x86_64 -yyum install epel-release.noarch -yyum install expat.x86_64 -yyum install filesystem.x86_64 -yyum install gamin.x86_64 -yyum install gdbm.x86_64 -yyum install gmp.x86_64 -yyum install gpgme.x86_64 -yyum install groff.x86_64 -yyum install info.x86_64 -yyum install initscripts.x86_64 -yyum install iproute.x86_64 -yyum install kernel.x86_64 -yyum install kernel.x86_64 -yyum install kernel.x86_64 -yyum install kernel-devel.x86_64 -yyum install kernel-devel.x86_64 -yyum install kernel-firmware.noarch -yyum install kernel-headers.x86_64 -yyum install libacl.x86_64 -yyum install libattr.x86_64 -yyum install libcap.x86_64 -yyum install libcurl.x86_64 -yyum install libffi.x86_64 -yyum install libgcrypt.x86_64 -yyum install libgpg-error.x86_64 -yyum install libidn.x86_64 -yyum install libsepol.x86_64 -yyum install libssh2.x86_64 -yyum install libtasn1.x86_64 -yyum install libxml2.x86_64 -yyum install lua.x86_64 -yyum install make.x86_64 -yyum install mysql-libs.x86_64 -yyum install ncurses.x86_64 -yyum install ncurses-base.x86_64 -yyum install ncurses-libs.x86_64 -yyum install nspr.x86_64 -yyum install nss.x86_64 -yyum install nss-softokn.x86_64 -yyum install nss-softokn-freebl.x86_64 -yyum install nss-sysinit.x86_64 -yyum install nss-tools.x86_64 -yyum install nss-util.x86_64 -yyum install openssh.x86_64 -yyum install openssh-clients.x86_64 -yyum install openssh-server.x86_64 -yyum install openssl.x86_64 -yyum install openssl-devel.x86_64 -yyum install p11-kit.x86_64 -yyum install p11-kit-trust.x86_64 -yyum install pcre.x86_64 -yyum install perl.x86_64 -yyum install perl-Module-Pluggable.x86_64 -yyum install perl-Pod-Escapes.x86_64 -yyum install perl-Pod-Simple.x86_64 -yyum in